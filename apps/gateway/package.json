{
  "name": "fpc-gateway",
  "version": "1.0.0",
  "description": "FPC API gateway for dockerized execution",
  "main": "dist/index.js",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc && concurrently \"tsc --watch\" \"nodemon -q dist/index.js\""
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "18.11.18",
    "concurrently": "^7.6.0",
    "eslint": "^8.36.0",
    "eslint-config-custom": "workspace:*",
    "nodemon": "^2.0.21",
    "rimraf": "latest",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "typescript": "^5.0.2"
  },
  "files": [
    "dist",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ]
}
