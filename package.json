{
  "name": "flight-plan-clock",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "autorouterDtoBuild": "cd packages/autorouter-dto && pnpm run build:main",
    "fplClockDtoBuild": "cd packages/flight-plan-clock-dto && pnpm run build:main",
    "dtoBuild": "pnpm run autorouterDtoBuild && pnpm run fplClockDtoBuild",
    "autorouterDtoWatchBuild": "cd packages/autorouter-dto && pnpm run watch:build",
    "fplClockDtoWatchBuild": "cd packages/flight-plan-clock-dto && pnpm run watch:build",
    "dtoWatchBuild": "concurrently \"pnpm run autorouterDtoWatchBuild\" \"pnpm run fplClockDtoWatchBuild\"",
    "devMockGui": "cd apps/autorouter-mock-gui && pnpm run dev",
    "devMock": "cd apps/autorouter-mock && pnpm run dev",
    "devBackend": "cd apps/flight-plan-clock-backend && pnpm run dev",
    "devFrontend": "cd apps/flight-plan-clock-frontend && pnpm run dev",
    "devAll": "pnpm run dtoBuild && concurrently \"pnpm run dtoWatchBuild\" \"pnpm run devMock\" \"pnpm run devMockGui\" \"pnpm run devBackend\" \"pnpm run devFrontend\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^7.6.0"
  }
}
